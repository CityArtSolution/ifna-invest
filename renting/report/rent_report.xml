<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


    <!-- Inherit Form View to Modify it -->
    <record id="sale_rental_report_new_form_view" model="ir.ui.view">
        <field name="name">sale.rental.report.new.pivot</field>
        <field name="model">sale.rental.report</field>
        <field name="inherit_id" ref="sale_renting.rental_report_pivot_view"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='price']" position="after">
                <field name="property_id" type="measure"/>
                <field name="state_id" type="row"/>
                <field name="property_address_build" type="measure"/>
                <field name="property_address_city" type="measure"/>
                <field name="country" type="measure"/>
                <!-- Add your fields or attributes here -->
            </xpath>

        </field>
    </record>
    </data>
</odoo>