<odoo>

        <record id="tenants_report_wizard_form_views" model="ir.ui.view">
            <field name="name">Update Tenants Report</field>
            <field name="model">tenants.report.wizard</field>
            <field name="arch" type="xml">
                <form string="Commissions Report">
                    <sheet>
                        <group col="4" colspan="4">
                            <field name="date_from"/>
                            <field name="date_to"/>
                        </group>
                    </sheet>
                    <footer>
                        <button string="Print Report" name="print_xlsx_report" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                    </footer>

                </form>
            </field>
        </record>

        <record id="tenants_report_wizard_report" model="ir.actions.act_window">
            <field name="name">Updates Tenants Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">tenants.report.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="tenants_report_wizard_form_views"/>
            <field name="target">new</field>
        </record>

        <menuitem id="sale_renting.menu_rental_reporting" name="Reporting"
                  parent="sale_renting.rental_menu_root"/>

        <menuitem id="tenants_report_wizard_menu" name="Updates Tenants"
                  parent="sale_renting.menu_rental_reporting"
                  action="tenants_report_wizard_report" sequence="37"/>

        <menuitem id="rental_analysis_report_menu" name="Rental Analysis"
                  parent="sale_renting.menu_rental_reporting"
                  action="sale_renting.action_rental_report" sequence="36"/>

</odoo>